# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
!/web/libraries/
/web/libraries/*

# Ignore sensitive information
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore files generated by VSCode
/.vscode/

# Ignore .env files as they are personal
/.env

# Ignore .sql files as they are transfered by rsync
/*.sql
/*.sql.gz
/*.dump

# Ignore gems and sass-cache in theme
/web/themes/*/.vendor
/web/themes/*/.sass-cache


# Ignore devel service settings
/web/sites/*/services.yml
web/sites/development.services.yml

# Include libraries not generated by composer
!/web/libraries/jquery.cycle
!/web/libraries/jquery.hoverIntent
!/web/libraries/json2

# Ignore file with AWS secrets
/container/aws/*secrets.yaml

# Ignore local settings
/web/sites/settings.local.php

#Misc
/nbproject/
__test*
.*.swp
*~
/.editorconfig
/.gitattributes
.DS_Store
